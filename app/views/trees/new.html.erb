<div class="grid_16">
  <h1>New tree</h1>

  <% form_for(@tree) do |form| %>
    <%= form.error_messages %>

    <%= render :partial=>'location_fields', :object=>form%>
    <%= render :partial=>'tree_fields', :object=>form%>

    <p>
      <%= form.submit 'Create' %>
      <%= link_to 'Cancel', trees_path %>
    </p>
  <% end %>

</div>